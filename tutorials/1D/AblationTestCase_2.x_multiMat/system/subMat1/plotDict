FoamFile {
  version         5.0;
  format          ascii;
  class           dictionary;
  location        system/subMat1;
  object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat         raw;
surfaceFormat     raw;

sets
(
plot {
  type            points;
  ordered on;
  axis            xyz;
  points   (
      ( 0.005 1.34e-2 0.005 ) // TC 0
  );
}

);


fields
(
    Ta
);

