FoamFile {
  version     2.0;
  format      ascii;
  class       volTensorField;
  location    "0";
  object      materialDirections;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

// internalField   uniform ( 0.7071 0.7071 0
//               -0.7071 0.7071 0
//               0 0 1 );
// internalField   uniform ( 0 1 0
//               -1 0 0
//               0 0 1 );
internalField   uniform ( 1 0 0
                          0 1 0
                          0 0 1 );

boundaryField {
  left
  {
    type            symmetryPlane;
  }
  top
  {
    type            zeroGradient;
  }
  bottom
  {
    type            symmetryPlane;
  }
  right
  {
    type            zeroGradient;
  }
  front
  {
    type            zeroGradient;
  }
  back
  {
    type            symmetryPlane;
  }
}


