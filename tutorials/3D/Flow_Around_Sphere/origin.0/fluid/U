FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  location    "0/fluid";
  object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  inlet
  {
    type            fixedValue;
    value           uniform (0.0005 0 0);
  }
  outlet
  {
    type            zeroGradient;
  }
  left
  {
    type            zeroGradient;
  }
  right
  {
    type            zeroGradient;
  }
  front
  {
    type            zeroGradient;
  }
  back
  {
    type            zeroGradient;
  }
}


