FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      rho_s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1600;

boundaryField {
  top
  {
    type            zeroGradient;
  }
  inlet
  {
    type            calculated;
    value           uniform 1600;
  }

  outlet
  {
    type            calculated;
    value           uniform 1600;
  }
  wedge_neg
  {
    type            wedge;
  }
  wedge_pos
  {
    type            wedge;
  }
  center
  {
    type           empty;
  }
}


