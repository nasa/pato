boundaryField {
  top
  {
    type            calculated;
    value           uniform 2595;
  }
  bottom
  {
    type            calculated;
    value           uniform 2595;
  }
  wedge1
  {
    type            wedge;
  }
  wedge2
  {
    type            wedge;
  }
  porousMat_to_tank
  {
    type            zeroGradient;
  }
}


