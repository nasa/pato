FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/cylinder";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField {
  top
  {
    type        fixedValue;
    value       uniform 101325;
  }
  bottom
  {
    type        fixedValue;
    value 	    uniform 101324;
  }
  porousMat_to_tank
  {
    type            zeroGradient;
  }

#includeEtc "caseDicts/setConstraintTypes"
}


