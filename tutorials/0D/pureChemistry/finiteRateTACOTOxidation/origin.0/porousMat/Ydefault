FoamFile {
  version         2.0;
  format          ascii;
  class           volScalarField;
  object          Ydefault;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions          [ 0 0 0 0 0 0 0 ];

internalField   uniform 1.0;

boundaryField {
  top
  {
    type 		positiveGradient;
    gradient 	uniform 0;
    value		uniform 1;
  }
  sides
  {
    type            zeroGradient;
  }

  bottom
  {
    type            zeroGradient;
  }
}


