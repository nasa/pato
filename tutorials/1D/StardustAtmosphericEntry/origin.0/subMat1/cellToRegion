FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/subMat1";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    sides
    {
        type            zeroGradient;
    }
    subMat1_to_subMat2
    {
        type            calculated;
        value           uniform 0;
    }
    subMat1_to_porousMat
    {
        type            calculated;
        value           uniform 0;
    }
}


