FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/cube";
  object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField {
  defaultFaces
  {
    type            empty;
  }

  up
  {
    type            symmetryPlane;
  }
  down
  {
    type            symmetryPlane;
  }
  left
  {
    type            fixedValue;
    value           uniform 0.1;
  }
  right
  {
    type            zeroGradient;
    value           uniform 0;
  }
  flow_to_porousMat
  {
    type            compressible::kqRWallFunction;
    value           uniform 0.1;
  }
}


