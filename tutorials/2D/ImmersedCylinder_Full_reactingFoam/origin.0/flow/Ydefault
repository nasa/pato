FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField {
#include "Bc"
  left
  {
    type            fixedValue;
    value           $internalField ;
  }
  right
  {
    type            inletOutlet;
    inletValue      $internalField;
    value           $internalField ;
  }
  flow_to_porousMat
  {
    type            fixedValue;
    value           $internalField;
  }
}

