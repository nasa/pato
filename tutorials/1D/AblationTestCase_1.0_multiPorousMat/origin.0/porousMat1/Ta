FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      Ta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField {
  top
  {
    type uniformFixedValue;
    uniformValue table
    (
        (0   300)
        (0.1 1644)
        (60 1644)
        (60.1 300)
        (120 300)
    );
  }
  sides
  {
    type            zeroGradient;
  }
  porousMat1_to_porousMat2
  {
    type            compressible::turbulentTemperatureCoupledBaffleMixed;
    value           uniform 300;
    Tnbr            Ta;
    kappaMethod     lookup;
    kappa           k_abl_sym;
  }
}


