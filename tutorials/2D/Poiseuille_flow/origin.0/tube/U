FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  location    "0";
  object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {

  tube_inlet
  {
    type            flowRateInletVelocity;
    volumetricFlowRate    constant 1E-07;
    value           uniform (0 0 0);
  }


  tube_top
  {
    type           noSlip;
  }

  tube_outlet
  {
    type           inletOutlet;
    inletValue     uniform  (0 0 0)
    value          uniform (0  0 0);
  }


  tubewedge_neg
  {
    type            wedge;
  }

  tubewedge_pos
  {
    type            wedge;
  }

  emptycenter
  {
    type           empty;
  }



}


