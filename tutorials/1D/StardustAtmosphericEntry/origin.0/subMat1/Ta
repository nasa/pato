FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      Ta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 260;

boundaryField {
  sides
  {
    type            zeroGradient;
  }
  subMat1_to_subMat2
  {
    type            compressible::turbulentTemperatureCoupledBaffleMixed;
    value           uniform 260;
    Tnbr 		Ta;
    kappaMethod     lookup;
    kappa           k_abl_sym;
  }
  subMat1_to_porousMat
  {
    type            compressible::turbulentTemperatureCoupledBaffleMixed;
    value           uniform 260;
    Tnbr 		Ta;
    kappaMethod     lookup;
    kappa		k_abl_sym;
  }
}


