FoamFile {
  version     5.0;
  format      ascii;
  class       volVectorField;
  location    "0/porousMat";
  object      cellMotionU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  top
  {
    type            zeroGradient;
  }

  bottom
  {
    type            symmetryPlane;
  }

  left
  {
    type            symmetryPlane;
  }

  back
  {
    type            symmetryPlane;
  }

  right
  {
    type            zeroGradient;
  }

  front
  {
    type            zeroGradient;
  }
}


