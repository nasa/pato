FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/cube";
  object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField {
  up
  {
    type            symmetryPlane;
  }
  down
  {
    type            symmetryPlane;
  }
  left
  {
    type            totalPressure;
    gamma           1.4;
    p0              uniform 101330;
  }
  right
  {
    type            fixedValue;
    value           uniform 101325;
  }
  flow_to_porousMat
  {
    type            zeroGradient;
  }
}


