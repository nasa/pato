FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e3;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 1.01e5;
    }

    bottom
    {
        type           zeroGradient;
    }

    sides
    {
        type           zeroGradient;
    }

}

