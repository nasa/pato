FoamFile
{
    version         5.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          sampleDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat         raw;
surfaceFormat     raw;

sets
(
    plot
    {
      type            cloud;
      axis            xyz;
      points          (
		       (0.5 0.5 0.5)
		       );
    }

 );


fields
(
 Ta
 piTotal
 rho_s
);

