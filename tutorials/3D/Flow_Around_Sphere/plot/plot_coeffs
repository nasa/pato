set term x11 persist

set multiplot layout 1,2

set size 1, 0.5

set xlabel 'time'
set ylabel 'cd'
plot [1:][] 'postProcessing/forceCoeffs_object/0/forceCoeffs.dat' u 1:3 w l notitle


set size 1, 0.5
set origin 0, 0.5

set xlabel 'time'
set ylabel 'cl'
plot [1:][] 'postProcessing/forceCoeffs_object/0/forceCoeffs.dat' u 1:4 w l notitle

unset multiplot

pause 20
