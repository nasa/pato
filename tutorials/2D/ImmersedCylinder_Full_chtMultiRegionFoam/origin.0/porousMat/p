FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/cylinder";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.01325e5;

boundaryField {
  defaultFaces
  {
    type            empty;
  }

  porousMat_to_flow
  {
    type            fixedValueToNbrValue;
    nbr             p;
    value 	    $internalField;
  }
}


