FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      rho_s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField {
  ".*"
  {
    type            calculated;
    value uniform   1000;
  }
}


