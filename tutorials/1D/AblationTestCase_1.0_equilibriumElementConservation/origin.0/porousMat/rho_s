FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho_s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 300;

boundaryField
{
    top
    {
        type            calculated;
        value uniform 300;
    }
    sides
    {
        type            calculated;
        value uniform 300;
    }
    bottom
    {
        type            calculated;
        value uniform 300;
    }
}


