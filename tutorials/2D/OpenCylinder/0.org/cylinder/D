FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  location    "0/cylinder";
  object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  left
  {
    type            symmetryPlane;
    value           $internalField;

  }
  bottom
  {
    type            symmetryPlane;
    value           $internalField;
  }
  inside
  {
    type            tractionDisplacement;
    traction        uniform ( 0 0 0 );
    pressure        uniform 0;
    value           uniform (0 0 0);
  }
  outside
  {
    type            tractionDisplacement;
    traction        uniform ( 0 0 0 );
    pressure        uniform 0;
    value           uniform (0 0 0);
  }

  frontAndBack
  {
    type            empty;
  }
}


