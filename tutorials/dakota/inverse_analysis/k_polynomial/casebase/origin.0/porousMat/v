FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  location    "0/porousMat";
  object      v;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  top
  {
    type            calculated;
    value           uniform (0 0 0);
  }
  sides
  {
    type            calculated;
    value           uniform (0 0 0);
  }
  bottom
  {
    type            calculated;
    value           uniform (0 0 0);
  }
}


