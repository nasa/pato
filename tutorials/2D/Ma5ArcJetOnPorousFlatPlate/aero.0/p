FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1.84e+06;

boundaryField {
  top
  {
    type            zeroGradient;
  }
  outlet
  {
    type            waveTransmissive;
    gamma           1;
    fieldInf        3750;
    lInf            0.025;
    value           uniform 3750;
  }
  front_bottom
  {
    type            zeroGradient;
  }
  back_bottom
  {
    type            zeroGradient;
  }
  left_wall
  {
    type            zeroGradient;
  }
  obstacle
  {
    type            zeroGradient;
  }
  materiau
  {
    type            zeroGradient;
  }
  inlet1
  {
    type            totalPressure;
    rho             rho;
    psi             none;
    gamma           1;
    p0              uniform 1.84e+06;
    value           uniform 1.84e+06;
  }
  nozzle
  {
    type            zeroGradient;
  }
  top_nozzle
  {
    type            zeroGradient;
  }
  bottom_nozzle
  {
    type            zeroGradient;
  }
  defaultFaces
  {
    type            empty;
  }
}


