FoamFile
{
    version         7.0;
    format          ascii;
    class           dictionary;
    location        system/porousMat;
    object          plotDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat         raw;
surfaceFormat     raw;

sets
(
    plot
    {
      type            points;
      axis            xyz;
      ordered         on;
      points          (
		       (0 0.049 0)                                                                                                                                                                                                                          
		       (0 0.048 0)                                                                                                                                                                                                                          
		       (0 0.046 0)                                                                                                                                                                                                                          
		       (0 0.042 0)                                                                                                                                                                                                                          
		       (0 0.038 0)                                                                                                                                                                                                                          
		       (0 0.034 0)                                                                                                                                                                                                                          
		       (0 0.026 0)
		       );
    }
);


fields
(
    Ta
    rho_s
);

