FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/cylinder";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField {
  left
  {
    type            symmetryPlane;
    value           $internalField;
  }
  bottom
  {
    type            symmetryPlane;
    value           $internalField;
  }
  inside
  {
    type            zeroGradient;
    value           $internalField;
  }
  outside
  {
    type            zeroGradient;
    value           $internalField;
  }

#includeEtc "caseDicts/setConstraintTypes"
}


