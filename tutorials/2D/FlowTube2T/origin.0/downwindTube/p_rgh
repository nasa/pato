FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField {

  downwindTube_outlet
  {

    type          fixedValue;
    value         uniform  101325;
  }

  downwindTube_top
  {
    type            zeroGradient;
  }


  downwindTube_to_porousMat
  {
    type            zeroGradient;
  }

  downwindTube_neg
  {
    type            wedge;
  }

  downwindTube_pos
  {
    type            wedge;
  }

  emptycenter2
  {
    type           empty;
  }

}


