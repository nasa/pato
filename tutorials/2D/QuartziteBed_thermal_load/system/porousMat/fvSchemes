FoamFile {
  version     2.0;
  format      ascii;
  class       dictionary;
  object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes {
default         Euler;
}

ddtSchemes {
default         Euler;
}

gradSchemes {
default         Gauss linear;
  grad(p)         Gauss linear;
  grad(D)         leastSquares;
}

divSchemes {
default         Gauss linear;
}

laplacianSchemes {
default         none;
  laplacian(DD,D)      Gauss linear uncorrected;
}

interpolationSchemes {
default         linear;
}

snGradSchemes {
default         uncorrected;
}

