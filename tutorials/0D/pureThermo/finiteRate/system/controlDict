FoamFile {
  version     4.0;
  format      ascii;
  class       dictionary;
  object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     PATOx;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         120;

deltaT          0.1;

writeControl    adjustableRunTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

graphFormat     xmgr;

runTimeModifiable yes;

adjustTimeStep  yes;

adjustStartTime 0;

maxCo           10;

maxDeltaT   1;

minDeltaT   1e-6;

