  outlet
  {
    type            zeroGradient;
  }

  inlet
  {
    type            zeroGradient;
  }

  z1
  {
    type            empty;
  }
  z2
  {
    type            empty;
  }

  "wall.*"
  {
    type            cyclic;
  }
  solid1
  {
    type            zeroGradient;
  }
  solid2
  {
    type            zeroGradient;
  }
}

