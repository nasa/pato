FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      Ta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 100;

boundaryField {
  top
  {
    type            zeroGradient;
  }

  bottom
  {
    type            symmetryPlane;
  }

  left
  {
    type            symmetryPlane;
  }

  back
  {
    type            symmetryPlane;
  }

  right
  {
    type            zeroGradient;
  }

  front
  {
    type            zeroGradient;
  }
}


