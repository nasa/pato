FoamFile {
  version         4.0;
  format          ascii;
  class           dictionary;
  location        system/porousMat2;
  object          surfacePatchDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPatchConstrained;

setFormat         raw;
surfaceFormat     raw;

sets
(
surfacePatch {
  type            boundaryPoints;
  axis            xyz;
  points        (
      (0 0.01 0)
  );
  maxDistance     1e-3;
  patches         ("porousMat2_to_porousMat1");
}
);

fields
(
    p
);

