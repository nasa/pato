FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/porousMat";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField {
  porousMat2_to_porousMat1
  {
    type            coupledMixed;
    value           uniform 101325;
    Tnbr            p;
    kappaMethod     lookup;
    kappa           Gamma_symm;
  }
  sides
  {
    type            zeroGradient;
  }
  bottom
  {
    type            zeroGradient;
  }
}


