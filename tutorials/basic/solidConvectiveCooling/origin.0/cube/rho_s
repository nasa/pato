FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      rho_s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 2595;

boundaryField {
  surface
  {
    type    zeroGradient;
    value   $internalField;
  }

  bottom
  {
    type    zeroGradient;
    value   $internalField;
  }

  frontAndBack
  {
    type   empty;
    value  $internalField;
  }
}

