FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField {
  topWall
  {
    type            zeroGradient;
  }
  bottomWall
  {
    type            zeroGradient;
  }
  outlet
  {
    type            totalPressure;
    p0              uniform 0;
    U               U;
    phi             phi;
    rho             none;
    psi             none;
    gamma           1;
    value           uniform 0;
  }
  inlet
  {
    type            zeroGradient;
  }
  frontAndBackPlanes
  {
    type            empty;
  }
  fluid_to_solid
  {
    type            zeroGradient;
  }

}


