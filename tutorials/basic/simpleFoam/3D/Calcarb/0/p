  x_min
  {
    type            zeroGradient;
  }
  x_max
  {
    type            fixedValue;
    value           uniform 0;
  }
  wall
  {
    type            zeroGradient;
  }
  y_min
  {
    type            zeroGradient;
  }
  y_max
  {
    type            zeroGradient;
  }
  z_min
  {
    type            zeroGradient;
  }
  z_max
  {
    type            zeroGradient;
  }
  fibres
  {
    type            zeroGradient;
  }
}


