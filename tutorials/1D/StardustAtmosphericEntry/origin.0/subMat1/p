FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    sides
      {
        type zeroGradient;
      }
    subMat1_to_subMat2
      {
	type zeroGradient;
      }
    subMat1_to_porousMat
      {
	type zeroGradient;
      }
}


