FoamFile {
  version         4.0;
  format          ascii;
  class           dictionary;
  location        system/porousMat1;
  object          plotDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat         raw;
surfaceFormat     raw;

sets
(
    plot
    {
      type            points;
      axis            xyz;
      ordered         on;
      points          (
		       (0 0.015 0)
		       );
    }
);

fields
(
    Ta
);

