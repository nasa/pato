FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0/porousMat";
  object      qRad;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;


boundaryField {
  top
  {
    type            boundaryMapping;
    mappingType constant;
    mappingFileName "constant/MISP1_qRad";
    updateTolTime_ 0.1;
    mappingFields ((qRad  "1"));
    value           uniform 0;
  }
  sides
  {
    type            zeroGradient;
  }
  bottom
  {
    type            zeroGradient;
  }
}

