FoamFile {
  version     4.0;
  format      ascii;
  class       dictionary;
  location    "constant/tank";
  object      tankProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

/****************************** IO *****************************************/
IO {
  writeFields();
  probingFunctions();
}
/****************************** END IO ************************************/

/****************************** MASS, ENERGY, PYROLYSIS *******************/
MaterialProperties {
  MaterialPropertiesType      PureConduction_UQ;
  MaterialPropertiesDirectory "constant/tank";
  detailedSolidEnthalpies     no;
}

Pyrolysis {
  PyrolysisType no;
}

Mass {
  MassType no;
}

Energy {
  EnergyType PureConduction;
}

SolidMechanics {
  SolidMechanicsType no;
  planeStress        no;
}
/****************************** END MASS, ENERGY, PYROLYSIS ****************/

/****************************** GAS PROPERTIES  ************************************/
GasProperties {
  GasPropertiesType no;
}
/****************************** END GAS PROPERTIES **************************/

/****************************** TIME CONTROL  **********************************/
TimeControl {
  TimeControlType           GradP;
  chemTransEulerStepLimiter no;
}
/****************************** END TIME CONTROL  ******************************/
