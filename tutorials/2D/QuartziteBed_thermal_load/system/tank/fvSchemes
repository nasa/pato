FoamFile {
  version     2.0;
  format      ascii;
  class       dictionary;
  object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes {
default         Euler;
}

ddtSchemes {
default Euler;
}

gradSchemes {
default Gauss linear uncorrected;
  grad(D) leastSquares;
}

divSchemes {
default Gauss linear uncorrected;
}

laplacianSchemes {
default              none;
  laplacian(k,Ta)    Gauss linear uncorrected;
  laplacian(DD,D)      Gauss linear uncorrected;
}

interpolationSchemes {
default linear;
}

snGradSchemes {
default uncorrected;
}


