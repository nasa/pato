FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField {

  upwindTube_inlet
  {

    type            calculated;
    value           $internalField;
  }


  upwindTube_top
  {
    type            calculated;
    value           $internalField;
  }

  upwindTube_to_porousMat
  {

    type            calculated;
    value           $internalField;
  }

  upwindTubewedge_neg
  {
    type            wedge;
  }

  upwindTubewedge_pos
  {
    type            wedge;
  }

  emptycenter
  {
    type           empty;
  }

}




