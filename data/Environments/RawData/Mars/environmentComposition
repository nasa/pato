Yke[AR]                0.005;


// Finite-rate chemistry: Boundary layer edge species composition (in MASS fractions)
Yie[CO2]               0.97;
Yie[N2]                0.01;
Yie[AR] 	       0.015;
Yie[O2] 	       0.005;



