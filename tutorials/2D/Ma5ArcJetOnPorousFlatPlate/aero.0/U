  {
    type            noSlip;
  }
  outlet
  {
    type            zeroGradient;
  }
  front_bottom
  {
    type            noSlip;
  }
  back_bottom
  {
    type            noSlip;
  }
  left_wall
  {
    type            noSlip;
  }
  obstacle
  {
    type            noSlip;
  }
  materiau
  {
    type            noSlip;
  }
  inlet1
  {
    type            zeroGradient;
  }
  nozzle
  {
    type            noSlip;
  }
  top_nozzle
  {
    type            noSlip;
  }
  bottom_nozzle
  {
    type            noSlip;
  }
  defaultFaces
  {
    type            empty;
  }
}


