FoamFile {
  version         4.0;
  format          ascii;
  class           dictionary;
  location        system/porousMat;
  object          plotDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs ("libsampling.so");

interpolationScheme cellPoint;

setFormat         raw;
surfaceFormat     raw;

sets
(
plot {
  type            points;
  ordered on;
  axis            xyz;
  points  (
      (0 0.049 0)
      (0 0.048 0)
      (0 0.046 0)
      (0 0.042 0)
      (0 0.038 0)
      (0 0.034 0)
      (0 0.026 0)
  );
}
);


fields
(
    Ta
    Y[N2]
    Y[O2]
    Y[CO2]
    Y[CO]
    Y[H2O]
    Y[H2]
    Y[CH4]
    Y[A1]
    Y[A1OH]
    Y[C(gr)]
);

