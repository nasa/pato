FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  outlet
  {
    type            zeroGradient;
  }

  inlet
  {
    type            fixedValue;
    value           uniform (18 0 0);
  }

  "wall.*"
  {
    type            cyclic;
  }
  solid1
  {
    type            noSlip;
  }
  solid2
  {
    type            noSlip;
  }
  z1
  {
    type            empty;
  }
  z2
  {
    type            empty;
  }
}

