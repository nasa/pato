FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField {
  outlet
  {
    type            fixedValue;
    value           uniform 300;
  }

  inlet
  {
    type            fixedValue;
    value           uniform 300;
  }

  solid1
  {
    type            fixedValue;
    value           uniform 300;
  }
  solid2
  {
    type            fixedValue;
    value           uniform 300;
  }
  "wall.*"
  {
    type            cyclic;
  }
  z1
  {
    type            empty;
  }
  z2
  {
    type            empty;
  }
}

