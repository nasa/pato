FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

C
{
    type        uniform;
    value       2500;
}

k
{
    type        uniform;
    value       0.3;
}

alpha
{
    type        uniform;
    value       5.4e-05;
}

thermalStress   yes;


