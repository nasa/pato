FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions                  [1 -1 -2 0 0 0 0];

internalField               uniform 1e5;

boundaryField {
  hot
  {
    type                darcyFlowRatePressure;
    volumetricFlowRate  8.33e-3;
    value               uniform 1e5;
  }

  cold
  {
    type                fixedValue;
    value               uniform 1e5;
  }

#includeEtc "caseDicts/setConstraintTypes"
}

