FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      T_g;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 323;

boundaryField {
  top
  {
    type           zeroGradient;
  }

  inlet
  {
    type            fixedValue;
    value           uniform 293;
  }

  outlet
  {
    type            zeroGradient;
  }

  wedge_neg
  {
    type            wedge;
  }
  wedge_pos
  {
    type            wedge;
  }
  center
  {
    type           empty;
  }
}

