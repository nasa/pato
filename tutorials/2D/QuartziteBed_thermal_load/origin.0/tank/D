FoamFile {
  version     2.0;
  format      ascii;
  class       volVectorField;
  object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField {
  top
  {
    type            tractionDisplacement;
    traction        uniform (0 0 0);
    pressure        uniform 0;
    value           uniform (0 0 0);
  }

  bottom
  {
    type            tractionDisplacement;
    traction        uniform (0 0 0);
    pressure        uniform 0;
    value           uniform (0 0 0);
  }

  wall
  {
    type            tractionDisplacement;
    traction        uniform (0 0 0);
    pressure        uniform 0;
    value           uniform (0 0 0);
  }

  tank_to_porousMat
  {
    type            tractionDisplacement;
    traction        uniform (0 0 0);
    pressure        uniform 0;
    value           uniform (0 0 0);
  }

#includeEtc "caseDicts/setConstraintTypes"

}

