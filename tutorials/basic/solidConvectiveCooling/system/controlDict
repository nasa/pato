// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     PATOx;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         43200;

deltaT          5e-2;

writeControl    adjustableRunTime;

writeInterval   3600;

purgeWrite      0;

writeFormat     ascii;

writePrecision   6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

adjustStartTime 0;

revlength      1e3;

delaySolid     0;

